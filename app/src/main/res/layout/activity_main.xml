<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btnIWantHelp"
        android:layout_width="122dp"
        android:layout_height="105dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="132dp"
        android:layout_marginTop="275dp"
        android:src="@drawable/INeedHelp" />

    <ImageButton
        android:id="@+id/btnSomeoneNeedsHelp"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_alignEnd="@+id/btnIWantHelp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="-6dp"
        android:src="@drawable/common_google_signin_btn_icon_dark_normal_background" />


</RelativeLayout>